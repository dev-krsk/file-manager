{
    "name": "dev-krsk/file-manager",
    "type": "symfony-bundle",
    "description": "File Manager bundle",
    "keywords": [
        "symfony",
        "bundle",
        "file manager"
    ],
    "homepage": "https://github.com/dev-krsk/file-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Yurinskiy",
            "email": "yuriyyurinskiy@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "^4.4||^5.0",
        "symfony/security": "^4.4||^5.0",
        "symfony/yaml": "^4.4||^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dev\\Krsk\\FileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dev\\Krsk\\FileManager\\Tests\\": "tests/"
        }
    }
}
